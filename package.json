	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "npm run build:lib",
		"build:lib": "run-s build:lib:core build:lib:auth build:lib:dashboard build:lib:data-table build:lib:form-builder build:lib:entity-manager build:lib:users-manager",
		"build:lib:core": "ng build core --prod",
		"build:lib:auth": "ng build auth --prod",
		"build:lib:dashboard": "ng build dashboard --prod",
		"build:lib:form-builder": "ng build form-builder --prod",
		"build:lib:data-table": "ng build data-table --prod",
		"build:lib:entity-manager": "ng build entity-manager --prod",
		"build:lib:users-manager": "ng build users-manager --prod",
		"publish": "run-s publish:lib:core publish:lib:auth publish:lib:dashboard publish:lib:data-table publish:lib:form-builder publish:lib:entity-manager publish:lib:users-manager",
		"publish:lib:core": "npm publish dist/core --access public",
		"publish:lib:auth": "npm publish dist/auth --access public",
		"publish:lib:dashboard": "npm publish dist/dashboard --access public",
		"publish:lib:form-builder": "npm publish dist/form-builder --access public",
