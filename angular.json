{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "panel": {
      "projectType": "library",
      "root": "projects/panel",
      "sourceRoot": "projects/panel/src",
      "prefix": "k",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/panel/tsconfig.lib.json",
            "project": "projects/panel/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/panel/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/panel/src/test.ts",
            "tsConfig": "projects/panel/tsconfig.spec.json",
            "karmaConfig": "projects/panel/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/panel/tsconfig.lib.json",
              "projects/panel/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "0d704465-36d9-4f45-bf8e-6353907e5778"
  },
  "defaultProject": "panel"
}
